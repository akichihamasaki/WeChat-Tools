<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="jldp.portal.modules.demo.dao.BaseLogsMapper">
    <select id="selectBaseLogs" resultType="map" parameterType="jldp.portal.modules.security.model.BaseLogs">
        select * from base_logs where log_id = #{id}
    </select>
    <select id="findBaseLogs" resultType="map" parameterType="jldp.portal.modules.security.model.BaseLogs">
        select * from base_logs
    </select>
</mapper>
